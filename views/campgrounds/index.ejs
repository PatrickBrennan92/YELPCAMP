<% layout("/layouts/boilerplate.ejs") %>
    <h1>All Campgrounds</h1>
    <div>
        <a href="/campgrounds/new">New Camp</a>
    </div>
    <ul>
        <% for(c of campgrounds) { %>
            <li>
                <a href="/campgrounds/<%=c._id%>">
                    <%=c.title%>
                </a>,
                <%=c.location%> - $
                    <%=c.price%>
            </li>
            <% } %>
    </ul>